first_name = input("What is your first name?   ")
print("Hello, ", first_name)
if first_name == "Nadia":
    print(first_name, "is learning Python")
elif first_name == "Yan":
    print(first_name, "is learning python in community!")
else:
    age = int(input("How old are you? "))
    if age <= 6:
        print("Wow if you are confident with your reading {}, you are in!".format(first_name))
    print("You should start learning Python, {}.".format(first_name))
print("Have a good day, {}!".format(first_name))    
